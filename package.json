{
  "name": "nulladata",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.5.0",
    "@types/nodemailer": "^7.0.0",
    "@types/pdfkit": "^0.17.2",
    "@types/markdown-it": "^14.0.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "jsdom": "^26.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.2.5",
    "prisma": "^5.16.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "bullmq": "^5.7.8",
    "dayjs": "^1.11.11",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "markdown-it": "^14.0.0",
    "nodemailer": "^6.9.11",
    "pdfkit": "^0.15.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  }
}
